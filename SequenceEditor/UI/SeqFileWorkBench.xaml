<UserControl x:Class="TriCheer.Phoenix.SeqEditor.SeqFileWorkBench"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:TriCheer.Phoenix.SeqEditor"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <TabControl ItemsSource="{Binding SequenceFiles}">
            <TabControl.ItemTemplate>
                <DataTemplate>
                    <local:SequenceFileTab></local:SequenceFileTab>
                </DataTemplate>
            </TabControl.ItemTemplate>
        </TabControl>
            
    </Grid>
</UserControl>
